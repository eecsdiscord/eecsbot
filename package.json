{
  "name": "eecsbot",
  "version": "2.0.1",
  "license": "ISC",
  "author": "feefs",
  "main": "./dist/bot.js",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.ts\"",
    "predev": "rm -rf ./dist",
    "dev": "concurrently -n watch,node \"tsc-watch --noClear\" \"npx wait-on ./dist && node --inspect ./dist/bot.js\"",
    "dev:hot": "tsc-watch --onSuccess \"node --inspect ./dist/bot.js\"",
    "build": "rm -rf ./dist && tsc",
    "serve": "node ./dist/bot.js"
  },
  "dependencies": {
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "^3.1.3",
    "@sapphire/plugin-logger": "^3.0.1",
    "@sapphire/plugin-subcommands": "^3.2.3",
    "@sapphire/utilities": "^3.11.0",
    "@skyra/jaro-winkler": "^1.1.0",
    "better-sqlite3": "^7.6.2",
    "colorette": "^2.0.19",
    "discord.js": "^13.12.0",
    "dotenv": "^16.0.3",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.8.0"
  },
  "devDependencies": {
    "@sapphire/prettier-config": "^1.4.4",
    "@sapphire/ts-config": "^3.3.4",
    "@types/better-sqlite3": "^7.6.2",
    "@types/node-cron": "^3.0.5",
    "@types/nodemailer": "^6.4.6",
    "concurrently": "^7.5.0",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1"
  }
}
